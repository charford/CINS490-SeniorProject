<h1>Listing evaluators</h1>

<table>
  <tr>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @evaluators.each do |evaluator| %>
  <tr>
    <td><%= evaluator.user_id %></td>
    <td><%= link_to 'Show', evaluator %></td>
    <td><%= link_to 'Edit', edit_evaluator_path(evaluator) %></td>
    <td><%= link_to 'Destroy', evaluator, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

