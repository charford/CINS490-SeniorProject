<h3>Evaluators</h3>
<%= render 'evaluators/form' %>
<div id="table_border">
<table>
  <thead>
    <tr>
      <th style="width: 50%">User</th>
      <th></th>
      <th>Created</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @evaluators.each do |evaluator| %>
      <tr>
        <td><%= link_to User.find(evaluator.user_id), User.find(evaluator.user_id) %></td>
        <td></td>
        <td><%= evaluator.created_at.strftime("%m/%d/%Y") %></td>
        <td></td>
        <td></td>
        <td style='text-align: right'><%= link_to 'Destroy', evaluator, confirm: 'Are you sure?', method: :delete %></td>
      </tr>
    <% end %>
  </tbody>   
</table> 
</div>
<%= will_paginate  @evaluators, :param_name => 'eval_page' %>
